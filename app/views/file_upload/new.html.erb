<% provide(:title, "File Upload") %>
<div class="row">
<div class = "col-md-6 col-md-offset-3 ">
<%= form_tag url_for(:controller => 'file_upload', :action => 'show_data') , :multipart => true do %>
<%= label_tag 'Pick Module Name'  %>
 <%= select_tag 'module', options_for_select(["...","Admin","API","Changes","CMDB","Contracts","Dashboard","Elasticsearch","Problems","Releases","Reports","Solution","Tickets"], "...") ,class: 'form-control'%><br>

<%= label_tag 'Deployment Date' %><br>
<%= text_field_tag :start_date, Time.now.strftime("%m/%d/%Y") , class: 'form-control'%><br><br>
 <%= label_tag 'Attach File' %>
 
    <%= file_field_tag "file" ,  class: "form-control",required:'required'%><br><br>
  <%= submit_tag "Save", class: "btn btn-lg btn-primary" %><br>
<% end %>

</div>
</div>
<script type="text/javascript">
   $(document).ready(function(){
    $('#start_date').datepicker();
  });

</script>
